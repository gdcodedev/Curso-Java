package fundamentos;

public class TipoString2 {
	
	public static void main(String[] args) {
		
		var nome = "Guilherme";
		var sobrenome = "Cardoso";
		var idade = 29;
		var salario = 2500; 
		
		System.out.printf("O Senhor %s %s Tem  %d anos e ganha  R$ %f.", nome, sobrenome, idade, salario );
	}
	


}
