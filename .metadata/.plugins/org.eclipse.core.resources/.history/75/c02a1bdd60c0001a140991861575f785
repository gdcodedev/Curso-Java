package fundamentos;

public class Temperatura {
	
	public static void  main(String[] args) {
		
		// (F - 32) X 5/9 = C
		
		final double FATOR = 5.0 / 9.0;
		final double AJUSTE = 32;
		
		double farenheit = 86;
		double celsius = (farenheit - AHYSTE ) * FATOR;
		
		System.out.println(Celsius);
	
	
	
	
			
		
		
		
	}

}
